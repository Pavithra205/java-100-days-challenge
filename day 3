reverse a string
import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str=s.nextLine();
        String re="";
        for(int i=str.length()-1;i>=0;i--){
            re+=str.charAt(i);
        }
        System.out.print(re);
    }
}
